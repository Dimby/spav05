{
  "name": "sp5",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": " node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "start:dev": "dotenv -e .env.local -- ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npm run migrate:save && npm run migrate:up",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "seed": "ts-node --dir prisma --transpile-only seed",
    "studio": "prisma studio --experimental"
  },
  "author": "Keke Tantely <keketantely@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@nexus/schema": "0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/ioredis": "^4.17.0",
    "@types/lodash": "^4.14.158",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.13",
    "@types/request": "^2.48.5",
    "dotenv-cli": "^3.2.0",
    "prisma": "^2.20.1",
    "ts-node-dev": "1.0.0-pre.50",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "@types/axios": "^0.14.0",
    "apollo-errors": "^1.9.0",
    "apollo-server": "2.14.3",
    "apollo-server-express": "2.16.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bson-objectid": "^1.3.1",
    "bwip-js": "^2.0.11",
    "cors": "^2.8.5",
    "express": "4.x.x",
    "graphql": "14.6.0",
    "graphql-redis-subscriptions": "^2.2.2",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.17.3",
    "jest": "^26.6.3",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "nexus-plugin-prisma": "^0.16.1",
    "pg": "^8.5.1",
    "save": "^2.4.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:fix && npm run build"
    ],
    "prisma/seed/**/*.ts": [
      "npm run lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
